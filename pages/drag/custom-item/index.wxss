.item {
  position: relative;
  padding-top: 100%;
  background: #ffffff;
}
.item > view {
  border: 1rpx solid #ebedf0;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;
  padding: 10rpx;
  box-sizing: border-box;
}
.item > view image {
  width: 100%;
  height: 100%;
}
