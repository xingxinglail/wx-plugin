@font-face {
  font-family: "iconfont";
  src: url("data:application/x-font-woff2;charset=utf-8;base64,") format("woff2");
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 28rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-arrow-left:before {
  content: "\e601";
}

.icon-arrow-double-left:before {
  content: "\e62f";
}

.icon-arrow-right:before {
  content: "\e602";
}

.icon-arrow-double-right:before {
  content: "\e600";
}

.picker {
  width: 600rpx;
  position: fixed;
  left: 50%;
  top: 45%;
  z-index: 999999;
  text-align: center;
  box-sizing: border-box;
  opacity: 0;
  transform: scale(0) translate3D(-50%, -50%, 0);
  border-radius: 20rpx;
  overflow: hidden;
  background: #fff;
}
.picker.picker-show {
  transition: transform 0s, opacity 0.3s;
  transform: scale(1) translate3D(-50%, -50%, 0);
  opacity: 1;
}
.picker.picker-hide {
  transition: transform 0s 0.3s, opacity 0.3s;
}
.picker .date-header {
  border-bottom: 1rpx solid #ebedf0;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  padding: 0 0.5rem;
  position: relative;
  box-sizing: border-box;
}
.picker .date-header .btn {
  box-sizing: border-box;
  padding: 0 20rpx;
  height: 80rpx;
  cursor: pointer;
  position: absolute;
  top: 0;
  font-size: 0;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
.picker .date-header .btn .iconfont {
  line-height: 80rpx;
  font-size: 40rpx;
  color: #3F82FD;
}
.picker .date-header .doubleLeft {
  left: 0;
}
.picker .date-header .left {
  left: 80rpx;
}
.picker .date-header .doubleRight {
  right: 0;
}
.picker .date-header .right {
  right: 80rpx;
}
.picker .date-header .month {
  font-size: 36rpx;
  font-weight: bold;
}
.picker .date-body {
  font-size: 30rpx;
}
.picker .date-body .tr {
  display: flex;
  align-items: center;
}
.picker .date-body .tr > .th {
  flex: 1;
}
.picker .date-body .tr > .th > .cell {
  display: inline-block;
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin: 6rpx;
  font-weight: bold;
}
.picker .date-body .tr > .td {
  flex: 1;
}
.picker .date-body .tr > .td > .cell {
  display: inline-block;
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin: 6rpx;
  cursor: pointer;
  border-radius: 50%;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  color: #333333;
}
.picker .date-body .tr > .td > .cell.otherMonth {
  color: #999999;
}
.picker .date-body .tr > .td > .cell.cur {
  background-color: #3F82FD !important;
  color: #ffffff;
}
.picker .date-body .tr > .td > .cell.disabled {
  color: #cccccc;
  background: #efefef;
  cursor: not-allowed;
}
.picker .time-header {
  border-bottom: 1rpx solid #ebedf0;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  position: relative;
  font-weight: bold;
}
.picker .time-header .date {
  font-size: 36rpx;
}
.picker .time-body {
  font-size: 30rpx;
}
.picker .time-body picker-view {
  width: 100%;
  height: 504rpx;
}
.picker .time-body picker-view .item {
  line-height: 72rpx;
}
.picker .time-body picker-view .selectItem {
  height: 72rpx;
}
.picker .bottom {
  display: flex;
  align-items: center;
  padding: 12rpx;
  border-top: 1rpx solid #ebedf0;
  box-sizing: border-box;
}
.picker .bottom .bd {
  flex: 1;
}
.picker .bottom .btn {
  padding: 20rpx;
  margin: -20rpx;
}
.picker .bottom .btn > view {
  width: 100rpx;
  padding: 8rpx 10rpx;
  text-align: center;
  border-radius: 8rpx;
  color: #ffffff;
}
.picker .bottom .btn-cancel > view {
  background: #999999;
  transition: background-color 0.3s;
}
.picker .bottom .btn-cancel > view.hover {
  background: rgba(153, 153, 153, 0.7);
}
.picker .bottom .btn-confirm > view {
  background: #3F82FD;
  transition: background-color 0.3s;
}
.picker .bottom .btn-confirm > view.hover {
  background: rgba(63, 130, 253, 0.7);
}

.modal {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 2;
  background: rgba(0, 0, 0, 0.6);
  opacity: 0;
  transform: translateY(100%);
}
.modal.modal-show {
  transition: transform 0s, opacity 0.3s;
  transform: translateY(0%);
  opacity: 1;
}
.modal.modal-hide {
  transition: transform 0s 0.3s, opacity 0.3s;
}
